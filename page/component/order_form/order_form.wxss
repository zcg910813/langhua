/*背景*/
.bg {  
  position: fixed;  
  top: 0%;  
  left: 0%;  
  width: 100%;  
  height: 100%;  
  background-color: black;  
  z-index: 9998;  
  -moz-opacity: 0.7;  
  opacity: 0.70;  
  filter: alpha(opacity=70);  
} 
