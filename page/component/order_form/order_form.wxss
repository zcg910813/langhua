@import '../../common/common.wxss';
.main{
  width:100%;
  height:100%;
  background: #f5f5f5;
  position:relative;
}
.avait{
  width:100%;
  height:90rpx;
  float:left;
  display: -webkit-flex; 
  border-bottom:1rpx solid #eee;
  line-height:90rpx;
  margin-bottom:20rpx;
  background: #fff;
  text-align: center;
  font-size:28rpx;
}
.avait  view{
  flex: 1;
}
.avait .active{
  color:#b42f2d;
  border-bottom:1rpx solid #b42f2d;
  box-sizing: border-box;
}
.message .right-item{
  display:none;
}
.message .active{
  display:block;
}
.obligation text:first-child{
    width:100%;
    height:88rpx;
    font-size:29rpx;
    text-align: center;
    float:left;
}

.header{
    position: relative;
    height: auto;
    line-height: 100rpx;
    padding:30rpx 30rpx 30rpx 150rpx;
    box-sizing: border-box;
    background: #AB956D;
    font-size: 28rpx;
    color: #fff;
}
.header .thumb{
    position: absolute;
    left: 30rpx;
    top: 30rpx;
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}
.header .about{
    float: right;
}

.address-box{
    width:100%;
    border-bottom: 0rpx solid #ededed;
    color: #999;
    line-height: 90rpx;
    font-size: 32rpx;
}
.address-box .address-manage{
    position: relative;
    height: 90rpx;
    border-bottom: 1rpx solid #e9e9e9;
    text-align:left;
    padding-left:10px;
}
.address-box .address-manage::after{
    position: absolute;
    right: 30rpx;
    top: 34rpx;
    content: '';
    width: 16rpx;
    height: 16rpx;
    border-top: 4rpx solid #7f7f7f;
    border-right: 4rpx solid #7f7f7f;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
}
.address-box .address-list{
    padding-left: 30rpx;
}
.address-box .address-list view{
    height: 90rpx;
    border-bottom: 1rpx solid #e9e9e9;
}
.address-box .address-list view:last-child{
    border-bottom: 0;
}

.orders-box{
    width: 100%;
    color: #999;
    float:left;
    background:#fff;
    height:240rpx;
}
.orders{
    height: 90rpx;
    line-height: 90rpx;
    border-bottom: 1rpx solid #e9e9e9;
    text-align: center;
}
.orders-list{
    width:100%;
    float:left;
    border-bottom: 20rpx solid #ededed;
    position:relative;
    background:#fff;
    padding-bottom: 5rpx;
    /* height:230rpx; */
}
.order_img{
  width:650rpx;
  height:120rpx;
  overflow: hidden;
  float:left;
  position: relative;
}
.order_img_a{
  width:750rpx;
  height:auto;
  overflow: hidden;
  float:left;
  position: relative;
}
.orders-list:last-child{
    border-bottom: 0;
}
.orders-number{
    height: 90rpx;
    line-height: 90rpx;
    border-bottom: 1rpx solid #e9e9e9;
    padding-left:10px;
    font-size:26rpx;
}
.orders-detail{
  width:102rpx;
  height: 102rpx;
  float: left;
  margin-top:10rpx;
  margin-right:10rpx;
}
.orders-detail image{
    width: 102rpx;
    height: 102rpx;
    float:left;
    border:1rpx solid #eee;
    margin-right:10rpx;
  
}
.orders-detail view{
    line-height: 60rpx;
}
.orders-detail .orders-status{
    position:absolute;
    right:30rpx;
    top:20rpx;
    height:120rpx;
    line-height:120rpx;
    color:#b42f2d;
    
}

.orders-detail_a{
position: relative;
width:365rpx;
height: 120rpx;
padding: 35rpx 1rpx 35rpx 200rpx;
border-bottom: 1rpx solid #e9e9e9;
}
.orders-detail_a image{
position: absolute;
left: 0;
top: 20rpx;
width: 150rpx;
height: 150rpx;
padding-left:10px;
}
.orders-detail_a view{
  width: 355rpx;
  line-height: 60rpx;
  font-size: 25rpx
}
.orders-detail_a .orders-status{
position: absolute;
right: 65rpx;
top: 70rpx;
height: 120rpx;
line-height: 120rpx;
color: #b42f2d;
font-size:24rpx; 
}

.orders-footer{
    height: 60rpx;
    line-height: 60rpx;
    color: #555;
    display:flex;
     position: relative; 
}
.orders-footer .orders-btn{
  position: absolute;
    width: 164rpx;
    height: 60rpx;
    line-height:60rpx;
    background: #b42f2d;
    color: #fff;
    top:-80rpx;
    right:160rpx;
    font-size:26rpx;
    text-align:center;
    border-radius:10rpx;

}
/*共多少件数*/
.order_total{
  width:135rpx;
  /* height:28rpx; */
  position:absolute;
  top:13rpx;
  right:5rpx;
  font-size:30rpx;
}
.order_total_a{
  width:135rpx;
  height:28rpx;
  position:absolute;
  bottom:0rpx;
  right:5rpx;
  font-size:30rpx;
}
/**状态大于3的按钮状态*/
.orders-footer .orders-status-btn-3{
    line-height:60rpx;
    background: #b42f2d;
    color: #fff;
    top:-80rpx;
    right:160rpx;
    font-size:26rpx

}
.orders-footer .orders-status-btn-3 button{
  background:#b42f2d;
  color:#fff;
}
/**状态8的按钮状态*/
.orders-footer .orders-status-8{
  float:right;
  width:60rpx;
  height:60rpx;
  line-height:60rpx;
}
/**购买按钮*/
.orders-footer .orders-buy-btn{
  float: right;
    width: 170rpx;
    height: 60rpx;
    line-height:60rpx;
    /* border-radius: 6rpx; */
    background:grey;
    color:#fff;
    padding-left:120px;
}
.orders-footer .orders-delete-btn{
  width:143rpx;
  height: 60rpx;
  position:absolute;
  top:-79rpx;
  right:12rpx;
  background: #b42f2d;
  color: #fff;
  font-size:26rpx;
  border-radius:10rpx;
  text-align: center;
}
.orders-footer .orders-text{
    float: right;
    height: 60rpx;
    line-height:60rpx;
    font-size:26rpx;
}
